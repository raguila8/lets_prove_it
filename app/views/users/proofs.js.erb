<% if @proofs.count == 0 %>
  $('#content2').html("<%= escape_javascript render(:partial => '/shared/no_results_found') %>");
<% else %>
$('#content2').html("<%= escape_javascript render(:partial => '/proofs/profile_proof_item', collection:  @proofs, as: :proof ) %>");

$content = $('#content2');
  setTimeout(function() {
	  MathJax.Hub.Queue(["Typeset", MathJax.Hub, $content[0]]);
} ,500);

$('.spinner').slideUp(3000);
$('#content2').attr("data-loaded", true);
<% end %>
