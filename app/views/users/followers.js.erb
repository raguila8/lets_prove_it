<% if @users.empty? %>
  $('#content3').html("<%= escape_javascript render(:partial => '/shared/no_results_found') %>");
<% else %>

  $('#content3').html("<%= escape_javascript render(:partial => '/users/user_item', collection:  @users, as: :user ) %>");

  $content = $('#content3');
    setTimeout(function() {
	    MathJax.Hub.Queue(["Typeset", MathJax.Hub, $content[0]]);
  } ,500);

  $('.spinner').slideUp(3000);
  $('#content3').attr("data-loaded", true);
<% end %>
