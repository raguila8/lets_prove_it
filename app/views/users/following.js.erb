<% if @users.empty? %>
  $('#content4').html("<%= escape_javascript render(:partial => '/shared/no_results_found') %>");
<% else %>

$('#content4').html("<%= escape_javascript render(:partial => '/users/user_item', collection:  @users, as: :user ) %>");

$content = $('#content4');
  setTimeout(function() {
	  MathJax.Hub.Queue(["Typeset", MathJax.Hub, $content[0]]);
} ,500);

$('.spinner').slideUp(3000);
$('#content4').attr("data-loaded", true);
<% end %>
